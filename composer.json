{
  "name": "magemonkey/rma",
  "description": "RMA (Return Merchandise Authorization) module for Magento 2.",
  "type": "magento2-module",
  "version": "1.0.0",
  "license": [
    "OSL-3.0",
    "AFL-3.0"
  ],
  "require": {
  "php": "~7.3.0||~7.4.0||~8.1.0",
  "magento/framework": ">=102.0.0 <=103.0.6",
  "magento/module-sales": ">=104.0.0 <=105.0.0"
  },
  "autoload": {
    "files": [ "registration.php" ],
    "psr-4": {
      "Magemonkey\\RMA\\": ""
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "./",
      "options": {
        "symlink": false
      }
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "map": [
      [
        "*",
        "Magemonkey/RMA"
      ]
    ]
  }
}
